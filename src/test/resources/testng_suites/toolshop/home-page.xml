    <test name="Checkbox Filter Test">
        <classes>
            <class name="com.zebrunner.carina.demo.toolshop.HomePageTest">
                <methods>
                    <include name="checkboxFilterTest"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>